/*
Author: Jakob Orel

16-bit Xor Gate: (Not16a)And16b Or16 aAnd16(Not16b)
*/

CHIP Xor16{
IN a[16], b[16];
OUT out[16];
PARTS:
Not16(in=a[0..15], out=notA);
And16(a=notA, b=b[0..15], out=o1);
Not16(in=b[0..15], out=notB);
And16(a=a[0..15], b=notB, out=o2);
Or16(a=o1, b=o2, out=out[0..15]);
}
